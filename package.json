{
	"name": "snowball",
	"displayName": "Snowball Language",
	"description": "Snowball language support for VS Code",
	"version": "0.0.6",
	"publisher": "MauroBalades",
	"engines": {
		"vscode": "^0.10.1"
	},
	"icon": "icon.png",
	"categories": [
		"Languages"
	],
    "galleryBanner": {
        "color": "#000",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/snowball/vscode-syntax.git"
    },
    "bugs": {"url": "https://github.com/snowball/vscode-syntax/issues"},
	"contributes": {
		"languages": [{
			"id": "snowball",
			"aliases": ["Snowball", "snowball"],
			"extensions": [".sn"],
			"configuration": "./snowball.configuration.json"
		}],
		"grammars": [{
			"language": "snowball",
			"scopeName": "source.snowball",
			"path": "./syntaxes/snowball.tmLanguage"
		}],
		"snippets": [
			{
				"language": "snowball",
				"path": "./snippets/snippets.json"
			}
		]
	}
}
